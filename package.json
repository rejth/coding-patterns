{
  "name": "coding-patterns",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "format:fix": "prettier  --write src/**/*.js",
    "lint:check": "eslint --debug src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "prepare": "husky install"
  },
  "author": "",
