  "lint-staged": {
    "*.js": [
      "npm run format:fix",
      "npm run lint:fix"
    ]
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.21",
    "prettier": "^3.0.3"
  }
}
